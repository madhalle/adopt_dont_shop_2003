<p><%= link_to "Pet Index", "/pets", method: "get"%></p>
<p><%= link_to "Shelter Index", "/shelters", method: "get"%></p>

<h1><%="#{@pet.name}"%></h1>

<%= image_tag(@pet.image, height:"400")%>
<p>Age: <%= @pet.age %></p>
<p>Sex: <%= @pet.sex %></p>
<p>Description: <%=@pet.description %></p>
<p>Adoption Status: <%= @pet.adoption_status %> </p>

<%=link_to "Update Pet", "/pets/#{@pet.id}/edit", method: "get"%>
<%=link_to "Delete Pet", "/pets/#{@pet.id}", method: "delete"%>
