<h4><%= link_to "Pet Index", "/pets", method: "get"%> </h4>
<h4><%= link_to "Shelter Index", "/shelters", method: "get"%> </h4>

<h1> Shelters </h1>

<div class="shelter-index">

<% @shelters.each do |shelter| %>
  <h2><%= link_to shelter.name, "/shelters/#{shelter.id}" %></h2>
  <h6><%= link_to "Edit #{shelter.name}", "/shelters/#{shelter.id}/edit", method: "get"%></h6>
  <h6><%= link_to "Delete #{shelter.name}", "/shelters/#{shelter.id}", method: "delete"%></h6>
<% end %>
</div>
<%= button_to "New Shelter", "/shelters/new", method: "get"%>
